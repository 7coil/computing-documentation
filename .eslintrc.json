{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "brackets"
    ],
	"env": {
		"node": true,
		"browser": true
	},
	"rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"comma-dangle": 0,
		"max-len": 0,
		"no-console": 0,
		"no-param-reassign": 0,
		"no-underscore-dangle": 0
	}
}
